<main class="container">
    <section class="projects__container">
        <h2 class="projects__title">{{projects.title}}</h2>
        <ul class="projects__list">
        {{#each projects.list}}
            <li class="projects__list-item">
                <a class="projects__list-item--demo" href="{{this.demoUrl}}">{{this.title}}</a>
                <a class="projects__list-item--source" href="{{this.sourceUrl}}">{{this.sourceMsg}}</a>
            </li>
        {{/each}}
        </ul>
    </section>
    <section class="skills__container">
        <h2 class="skills__title">{{skills.title}}</h2>
        <ul class="skills__list">
            {{#each skills.list}}
                <li class="skills__list-item"><span>{{this}}</span></li>
            {{/each}}
        </ul>
    </section>
</main>
