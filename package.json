{
  "name": "personal-website",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js",
    "build-task:scss-compile": "node-sass-chokidar --source-map true public/scss/ -o public/css",
    "build-task:autoprefixer": "postcss public/css/*.css --use autoprefixer -d public/css",
    "sass:build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar 'public/scss/**/*.scss' -c 'npm run sass:build'",
    "dev": "npm-run-all -p sass:*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.2"
  },
  "dependencies": {
    "autoprefixer": "^9.7.3",
    "chokidar-cli": "^2.1.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "mkdirp": "^0.5.1",
    "node-sass-chokidar": "^1.4.0",
    "node-sass-middleware": "^0.11.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3"
  }
}
